{
  "family": "lks-leantime-app",
  "taskRoleArn": "arn:aws:iam::950055801200:role/LabRole",
  "executionRoleArn": "arn:aws:iam::950055801200:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [ "FARGATE" ],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "leantime",
      "image": "leantime/leantime:latest",
      "cpu": 0,
      "essential": true,
      "command": [
        "sh",
        "-c",
        "sed -i 's/localhost/10.100.1.137/g' /var/www/html/config/configuration.php || true && /usr/bin/supervisord -c /etc/supervisor/conf.d/supervisord.conf"
      ],
      "portMappings": [
        { "containerPort": 8080, "hostPort": 8080, "protocol": "tcp" }
      ],
      "environment": [
        { "name": "LEANTIME_DB_HOST", "value": "lks-project-management-cluster.cfnkzpmutmbl.us-east-1.rds.amazonaws.com" },
        { "name": "LEANTIME_DB_USER", "value": "admin" },
        { "name": "LEANTIME_DB_PASSWORD", "value": "awskeren" },
        { "name": "LEANTIME_DB_DATABASE", "value": "mysql" },
        { "name": "LEANTIME_DB_PORT", "value": "3306" },
        { "name": "APP_PORT", "value": "8080" },
        { "name": "SITENAME", "value": "LKS Leantime" },
        { "name": "LEANTIME_DEBUG", "value": "1" },
        { "name": "LEANTIME_TIMEZONE", "value": "Asia/Jakarta" }
      ],
      "mountPoints": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks/leantime-app",
          "awslogs-region": "us-east-1",
          "awslogs-create-group": "true",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}