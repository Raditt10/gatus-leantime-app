{
  "family": "lks-leantime-app",
  "taskRoleArn": "arn:aws:iam::159908042030:role/LabRole",
  "executionRoleArn": "arn:aws:iam::159908042030:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [ "FARGATE" ],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "leantime",
      "image": "leantime/leantime:latest",
      "cpu": 0,
      "essential": true,
      "portMappings": [
        { "containerPort": 8080, "hostPort": 8080, "protocol": "tcp" }
      ],
      "secrets": [
        { "name": "LEAN_DB_HOST", "valueFrom": "/lks/db/host" },
        { "name": "LEAN_DB_USER", "valueFrom": "/lks/db/username" },
        { "name": "LEAN_DB_PASSWORD", "valueFrom": "/lks/db/password" },
        { "name": "LEAN_DB_DATABASE", "valueFrom": "/lks/db/name" },
        { "name": "LEAN_DB_PORT", "valueFrom": "/lks/db/port" }
      ],
      "environment": [
        { "name": "LEAN_PORT", "value": "8080" },
        { "name": "LEAN_SITENAME", "value": "LKS Leantime" },
        { "name": "LEAN_DEBUG", "value": "1" },
        { "name": "LEAN_DEFAULT_TIMEZONE", "value": "Asia/Jakarta" },
        { "name": "LEAN_SESSION_SECURE", "value": "false" },
        { "name": "LEAN_SESSION_PASSWORD", "value": "Radit123" }
      ],
      "mountPoints": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks/leantime-app",
          "awslogs-region": "us-east-1",
          "awslogs-create-group": "true",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}